{
  "name": "cycle-starter",
  "version": "0.1.0",
  "description": "Simple setup for cycle.js",
  "main": "",
  "scripts": {
    "build-jade": "./node_modules/.bin/jade ./src/index.jade -o ./dist/dev",
    "build-js": "./node_modules/.bin/webpack --progress --colors",
    "build": "npm run build-jade && npm run build-js",
    "watch-jade": "./node_modules/.bin/nodemon --watch ./src/app/index.jade --exec 'npm run build-jade'",
    "watch-js": "./node_modules/.bin/webpack --progress --colors --watch",
    "watch": "npm run watch-jade & npm run watch-js",
    "dev-server": "./node_modules/.bin/webpack-dev-server --content-base dist/dev/ --host 0.0.0.0 --inline --hot --progress --colors --watch",
    "test-server": "./node_modules/.bin/webpack-dev-server --hot 'mocha!./test/' --output-filename test.js",
    "build-prod": "./node_modules/.bin/webpack --config webpack.production.js -p --progress --colors",
    "build-prod-server": "./node_modules/.bin/webpack --config webpack.server.js --progress --colors",
    "preprod-server": "npm run build-prod && npm run build-prod-server",
    "prod-server": "node ./dist/server.js",
    "server": "npm run prod-server",
    "start": "npm run watch-jade & npm run dev-server",
    "test": "npm run test-server"
  },
  "author": "Tylor Steinberger",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-closure-elimination": "0.0.2",
    "babel-plugin-flow-comments": "^1.0.9",
    "babel-plugin-typecheck": "^1.3.0",
    "css-loader": "^0.19.0",
    "cssnext-loader": "^1.0.1",
    "cycle-history": "^0.3.4",
    "eslint": "^1.0.0",
    "eslint-config-cycle": "^3.0.0",
    "eslint-plugin-cycle": "^1.0.1",
    "eslint-plugin-no-class": "^0.1.0",
    "jade": "^1.11.0",
    "node-noop": "0.0.1",
    "nodemon": "^1.7.1",
    "style-loader": "^0.12.4",
    "stylus-loader": "^1.3.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "@cycle/core": "^3.1.0",
    "@cycle/dom": "^5.3.0",
    "cycle-history": "^0.3.5",
    "cycle-local-storage": "^1.0.0",
    "gsap": "^1.18.0",
    "rx-combine-latest-obj": "^1.0.2",
    "switch-path": "^1.1.0"
  }
}
