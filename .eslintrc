{
  "extends": "eslint-config-cycle",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "new-cap": 0,
    "arrow-spacing": 2,
    "object-curly-spacing": 0,
    "space-in-parens": 0,
    "quotes": [2, "backtick", "avoid-escape"],
    "semi": [2, "always"]
  }
}
